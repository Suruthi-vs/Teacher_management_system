<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Second Name</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody>
      {{#each teachermodel}}
      <tr>
        <th scope="row">{{formatIndex @index}}</th>
        <td>{{this.Firstname}}</td>
        <td>{{this.Lastname}}</td>
        <td>
          <a href="/edit/{{this.id}}">Edit</a>
          <a href="/delete/{{this.id}}">Delete</a>
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>